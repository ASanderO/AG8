#include <stdio.h>

    int main(void)

   {

       float num1, num2, soma, multiplicacao, divisao, subtracao;

       printf("Digite um numero\n");

        scanf("%f", &num1);

       printf("Digite outro numro\n");

        scanf("%f", &num2);

       soma = num1+num2;

        printf("A soma dos numeros: %f\n", soma);

		multiplicacao = num1*num2;

		 printf("A multiplicacao dos numeros: %f\n", multiplicacao);

		divisao = num1/num2;

		printf("A divisao dos numeros: %f\n", divisao);

		subtracao = num1-num2;

		printf("A subtracao dos numeros: %f\n", subtracao);

       return 0;

   }
